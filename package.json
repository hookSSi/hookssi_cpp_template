{
  "name": "hookssi_template",
  "version": "0.0.0-development",
  "description": "A github project template for future to use",
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "tests"
  },
  "private": true,
  "filse": [
    "CHANGELOG.md",
    "package.json",
    "package-lock.json"
  ],
  "scripts": {
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "commit": "git cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hookSSi/hookssi_template.git"
  },
  "keywords": [],
  "author": {
    "name": "hookssi",
    "email": "sounghoo12@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hookSSi/hookssi_template/issues"
  },
  "homepage": "https://github.com/hookSSi/hookssi_template#readme",
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^6.0.0",
    "semantic-release": "^17.4.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
